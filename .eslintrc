{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true,
    "jest/globals": true
  },
  "rules": {
    "semi": [2, "never"],
    "linebreak-style": "off",
    "operator-linebreak": "off",
    "indent": ["error", 4, { "SwitchCase": 1 }],
    "require-jsdoc": "off",
    "max-len": ["error", {"code":  120}]
  },
  "plugins": ["jest"],
  "extends": ["eslint:recommended", "google"]
}
